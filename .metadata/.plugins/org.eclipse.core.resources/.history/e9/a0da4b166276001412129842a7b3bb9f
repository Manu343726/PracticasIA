package aima.core.environment.spainmap;

public class Map {

	private City _actualCity;
	
	public Map(){
		_actualCity = new City();
	}
	
	public Map(City city){
		_actualCity = city;
	}
	
	public Map(Map map){
		_actualCity = map.getActualCity().clone();
	}
	
	public City getActualCity(){
		return _actualCity;
	}
	
}
